    "debug": "4.3.5",
    "dotenv": "16.4.5",
    "extract-zip": "2.0.1",
    "globby": "13.2.2",
    "graphql": "16.8.2",
    "graphql-request": "5.0.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.16.0",
