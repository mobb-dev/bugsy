    "graphql-request": "5.0.0",
    "inquirer": "9.2.7",
    "istextorbinary": "6.0.0",
    "nanospinner": "1.1.0",
    "node-fetch": "3.3.1",
    "octokit": "2.0.14",
    "open": "8.4.2",
    "semver": "7.5.0",
    "simple-git": "3.19.1",
    "snyk": "1.1118.0",
    "supports-color": "9.4.0",
    "tar": "6.2.0",
    "tmp": "0.2.1",
    "yargs": "17.7.2",
    "zod": "3.22.3"
